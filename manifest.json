{
    "manifest_version": 3,
    "name": "MindGuard",
    "version": "1.0",
    "action": {
    "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": ["storage", "tabs"],
    "background": {
        "service_worker": "background.js"
    }
    ,
    "host_permissions": [
        "http://10.0.0.20/*"
    ],
    "icons": {
        "64": "images/mindguard64.png",
        "128": "images/mindguard128.png"
    }
}